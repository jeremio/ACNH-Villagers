{
  "name": "list-villagers",
  "type": "module",
  "version": "2.1.0",
  "packageManager": "pnpm@10.28.2",
  "description": "Display villagers from Animal Crossing New Horizons with optimized filtering",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "oxlint": "oxlint",
    "oxlint:fix": "oxlint --fix",
    "analyze": "vite build --mode analyze",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "performance": "vite build --mode analyze && npm run preview",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "pinia": "^3.0.4",
    "vue": "^3.5.28",
    "vue-i18n": "^11.2.8",
    "vue-router": "^5.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@intlify/eslint-plugin-vue-i18n": "^4.1.1",
    "@intlify/unplugin-vue-i18n": "^11.0.7",
    "@playwright/test": "^1.58.2",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node24": "^24.0.4",
    "@types/node": "^24.10.13",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/language-server": "^3.2.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.3",
    "happy-dom": "^20.7.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "lucide-vue-next": "^0.575.0",
    "naive-ui": "^2.43.2",
    "oxlint": "^1.49.0",
    "rollup-plugin-visualizer": "^7.0.0",
    "typescript": "^5.9.3",
    "vfonts": "^0.0.3",
    "vite": "^7.3.1",
    "vite-plugin-vue-devtools": "^8.0.6",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  }
}
